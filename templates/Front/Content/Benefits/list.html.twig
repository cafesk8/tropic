<ul class="list-benefits {{ cssClass|default('') }}">
    <li class="list-benefits__item">
        <span class="list-benefits__item__icon">
            <i class="svg svg-exchange"></i>
        </span>
        <span class="list-benefits__item__text">
            {{ 'Vrácení zdarma <strong>do 30 dnů</strong>'|trans|raw }}
        </span>
    </li>
    <li class="list-benefits__item">
        <span class="list-benefits__item__icon">
            <i class="svg svg-delivery-truck"></i>
        </span>
        <span class="list-benefits__item__text">
            {{ 'Doprava zdarma při nákupu <strong>nad 2&nbsp;500&nbsp;Kč</strong>'|trans|raw }}
        </span>
    </li>
    <li class="list-benefits__item">
        <span class="list-benefits__item__icon">
            <i class="svg svg-towel"></i>
        </span>
        <span class="list-benefits__item__text">
            {{ 'Vyzkoušení zdarma na <a class="list-benefits__item__text__link" href="%url%">naší prodejně</a>'|transHtml({ '%url%': url('front_store_index') })|raw }}
        </span>
    </li>
</ul>
