<div class="js-pickup-place-search" data-pickup-place-autocomplete-url="{{ url('front_pickup_place_autocomplete') }}">
    <div class="box-popup-add__content__title">
        <span class="box-popup-add__content__title__text double-lined">
            {{ 'Vyberte pobočku'|trans }}
        </span>
    </div>

    <div class="box-pickup-place">
        <div class="box-pickup-place__search">
            <div class="box-pickup-place__search__text">
                <label class="input__label">
                    {{ 'Zadejte město nebo PSČ:'|trans }}
                </label>
            </div>
            <div class="box-pickup-place__search__input">
                <input
                type="text"
                placeholder="{{ 'např. Praha'|trans }}"
                class="js-pickup-place-city-post-code-autocomplete-input input"
                autocomplete="off"
                />
                <div class="box-pickup-place__search__input__button">
                    <button class="btn btn--success btn--with-input" type="button">
                        {{ 'Vyhledat'|trans }}
                    </button>
                </div>
            </div>
        </div>
        <div class="js-pickup-place-autocomplete-results box-pickup-place__results {% if pickupPlaces|length > 0 %}display-block{% endif %}">
            {% if pickupPlaces|length > 0 %}
                {% include '@ShopsysShop/Front/Inline/PickupPlace/autocompleteResult.html.twig' with {
                    pickupPlaces: pickupPlaces,
                    chosenPickupPlace: chosenPickupPlace
                } %}
            {% endif %}
        </div>
    </div>
</div>
