{% if breadcrumbItems is not null %}
    <div class="web__line">
        <div class="web__container">
    <div class="box-breadcrumb">
        <span class="box-breadcrumb__item box-breadcrumb__item--home">
            <i class="svg svg-home"></i>
        </span>
        <a href="{{ url('front_homepage') }}" class="box-breadcrumb__item underline">
            {{ 'Ãšvod'|trans }}
        </a>
        {% for breadcrumbItem in breadcrumbItems %}
            <span class="box-breadcrumb__item box-breadcrumb__item--separator {% if loop.last %} display-none{% endif %}"><i class="svg svg-triangle"></i></span>
            {% if breadcrumbItem.routeName is not null %}
                <a href="{{ url(breadcrumbItem.routeName, breadcrumbItem.routeParameters) }}" class="box-breadcrumb__item underline">
                    {{- breadcrumbItem.name -}}
                </a>
            {% else %}
                <span class='box-breadcrumb__item display-none'>
                    {{- breadcrumbItem.name -}}
                </span>
            {% endif %}
        {% endfor %}
    </div>
        </div>
    </div>
{% endif %}
